# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置 (选择一种)
DB_TYPE=supabase  # 可选: supabase, mysql, tencent, postgresql

# Supabase 配置
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# 腾讯云 MySQL 配置
TENCENT_DB_HOST=your_mysql_host
TENCENT_DB_PORT=3306
TENCENT_DB_USER=your_mysql_user
TENCENT_DB_PASSWORD=your_mysql_password
TENCENT_DB_NAME=wechat_mall

# PostgreSQL 配置
PG_HOST=your_postgres_host
PG_PORT=5432
PG_USER=postgres
PG_PASSWORD=your_postgres_password
PG_DATABASE=mall

# JWT 密钥
JWT_SECRET=your_jwt_secret_here_change_in_production

# 微信小程序配置
WECHAT_APPID=your_wechat_appid
WECHAT_SECRET=your_wechat_secret

# 文件上传配置
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760